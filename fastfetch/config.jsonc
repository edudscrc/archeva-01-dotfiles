{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "~/.config/fastfetch/arch.txt",
    "color": {
      "1": "yellow"
    },
    "padding": {
      "top": 1,
      "right": 6,
      "left": 4
    }
  },
  "display": {
    "separator": " "
  },
  "modules": [
    "break",
    {
      "type": "title",
      "key": ">",
      "keyColor": "blue",
      "color": {
        "host": "yellow",
        "at": "yellow",
        "user": "yellow"
      }
    },
    "break",
    {
      "type": "os",
      "key": ">",
      "keyColor": "blue",
      "outputColor": "yellow"
    },
    "break",
    {
      "type": "wm",
      "key": ">",
      "keyColor": "red",
      "outputColor": "blue"
    },
    {
      "type": "theme",
      "key": ">",
      "keyColor": "red",
      "outputColor": "blue"
    },
    "break",
    {
      "type": "cpu",
      "key": ">",
      "keyColor": "red",
      "outputColor": "blue"
    },
    {
      "type": "gpu",
      "key": ">",
      "keyColor": "red",
      "outputColor": "blue"
    },
    "break",
    {
      "type": "memory",
      "key": ">",
      "keyColor": "red",
      "outputColor": "cyan"
    },
    "break",
    {
      "type": "colors",
      "symbol": "circle"
    },
    "break"
  ]
}
